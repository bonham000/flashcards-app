{
  "name": "react-node-light-spa",
  "version": "1.0.1",
  "description": "React, Redux, Express.js",
  "engines": {
    "node": "6.8.1"
  },
  "main": "dist/server/index.js",
  "scripts": {
    "lint:server": "eslint src/server",
    "lint:client": "eslint src/client",
    "lint": "npm-run-all --silent lint:* || exit 0",
    "clean": "rm -rf dist/",
    "create:dist": "mkdir -p dist/client/ && mkdir -p dist/server/",
    "create:html": "cp src/client/index.html dist/client/",
    "predev": "npm-run-all create:*",
    "dev": "cross-env NODE_ENV=development nodemon --watch src/server --exec \"babel-node src/server/index.js\"",
    "prebuild": "npm-run-all lint clean create:*",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:server": "./node_modules/.bin/babel-node src/server -d dist/server",
    "build": "npm-run-all build:*",
    "start": "npm-run-all build:*"
  },
  "author": "Sean Smith",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-import-resolver-node": "^0.2.3",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.10.0",
    "nodemon": "^1.10.2",
    "postcss-loader": "^0.13.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.13.0",
    "bluebird": "^3.4.6",
    "cross-env": "^2.0.1",
    "compression": "^1.6.2",
    "dotenv": "^2.0.0",
    "npm-run-all": "^3.1.0",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "lodash": "^4.16.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "autoprefixer": "^6.4.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-import-resolver-node": "^0.2.3",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.10.0",
    "nodemon": "^1.10.2",
    "postcss-loader": "^0.13.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
